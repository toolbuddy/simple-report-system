<!-- Display redirect page, with specify message -->
<!DOCTYPE html>
<html>
<head>
<!-- Title & Icon -->
<title><%= title %></title>
<link rel="icon" href="https://i.imgur.com/nfeMkGD.png">
<% include ../elements/meta %>
<% include ../elements/lib %>
</head>
<body>

<div class="container font-fresca">
	<!-- Purpose -->
	<h2> <%= title %> </h2>
	<hr>
    <h1> <%= msg %> </h1>
    <input id="url" value="<%= url %>" hidden>
    <input id="duration" value="<%= duration %>" hidden>
</div>

<script>
$(document).ready(function() { 
    var url = $("#url").val()
    var duration = $("#duration").val()
    setTimeout(function(){
        window.location.assign(url);
    },duration*1000)
});
</script>

<!-- Footer -->
<hr>
<br>
<% include ../elements/normal_footer_bs %>
</html>